process {
  executor="local"
  withName: split_sample_sheet_task {
    executor="local"
  }
}
env {
  RUN_AGE=60
  BWA="/opt/common/CentOS_7/bwa/bwa-0.7.17/bwa"
  PICARD="java -jar /home/igo/resources/picard2.21.8/picard.jar"
  SEQUENCER_DIR="/igo/sequencers"
  FASTQ_DIR="/igo/work/FASTQ"
  STATS_DIR="/igo/stats"
  LAB_SAMPLE_SHEET_DIR="/pskis34/LIMS/LIMS_SampleSheets"
  SAMPLE_SHEET_DIR="/home/igo/SampleSheetCopies"
  PROCESSED_SAMPLE_SHEET_DIR="/igo/home/igo/DividedSampleSheets"
  LOG_FILE="$HOME/nf_fastq_run.log"
  SPLIT_SAMPLE_SHEETS="split_sample_sheets.txt"
  RUN_PARAMS_FILE="run_params.txt"
  RUNS_TO_DEMUX_FILE="Run_to_Demux.txt"
  PIPELINE_OUT="./pipeline_out"
  UNASSIGNED_PARAMETER="NOT_DETECTED"
  OUT_BWA="BWA"
  RUN_ERROR="RUN_ERROR"
}
